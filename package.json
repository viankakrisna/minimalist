{
  "name": "minimalist-boilerplate",
  "version": "1.0.0",
  "description": "Project boilerplate",
  "scripts": {
    "start": "node webpack.server",
    "build": "webpack --progress -p",
    "watch": "webpack --watch --progress -p",
    "deploy": "webpack --progress -p && cp -rf build/* . && git commit -am \"$m\" && git push origin master" 
  },
  "dependencies": {
    "preact": "7.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "html-webpack-plugin": "1.6.1",
    "webpack": "1.12.13",
    "webpack-dev-server": "^1.16.2"
  },
  "author": "Ade Viankakrisna Fadlil",
  "license": "ISC"
}
