{
  "name": "minimalist-boilerplate",
  "version": "1.0.0",
  "description": "Project boilerplate",
  "scripts": {
    "start": "node webpack.server",
    "build": "webpack --progress --optimize-minimize",
    "watch": "webpack --progress --optimize-minimize --watch",
    "deploy": "webpack --progress --optimize-minimize && cp -rf build/* . && git add --all && git commit -am \"$m\" && git push origin master"
  },
  "dependencies": {
    "offline-plugin": "^4.6.1",
    "preact": "8.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "7.1.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.0.0",
    "eslint-loader": "^1.6.1",
    "html-webpack-plugin": "2.29.0",
    "pwa-manifest-webpack-plugin": "^1.0.1",
    "webpack": "4.39.3",
    "webpack-dev-server": "^2.3.0"
  },
  "author": "Ade Viankakrisna Fadlil",
  "license": "ISC"
}
