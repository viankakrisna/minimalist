{
  "name": "minimalist-boilerplate",
  "version": "1.0.0",
  "description": "Project boilerplate",
  "scripts": {
    "start": "node webpack.server",
    "build": "webpack --progress --optimize-minimize",
    "watch": "webpack --progress --optimize-minimize --watch",
    "deploy": "webpack --progress --optimize-minimize && cp -rf build/* . && git add --all && git commit -am \"$m\" && git push origin master" 
  },
  "dependencies": {
    "preact": "7.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "html-webpack-plugin": "2.28.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "author": "Ade Viankakrisna Fadlil",
  "license": "ISC"
}
